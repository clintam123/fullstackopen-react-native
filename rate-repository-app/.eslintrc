{
    "plugins": [
        "react",
        "jest"
    ],
    "parser": "@babel/eslint-parser",
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "plugin:react/recommended",
        "plugin:jest/recommended",
        "standard"
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "off",
        "semi": "error"
    }
}